{
  "presets": [ [ "env", {
    "targets": {
      "browsers": [
        "last 2 versions",
        "ie >= 9"
      ]
    }
  } ] ],
  "plugins": [
    [
      "transform-react-jsx",
      {
        "pragma": "yoast._wp.element.createElement"
      }
    ],
    "dynamic-import-webpack",
    "transform-object-rest-spread"
  ],
  "comments": true
}
