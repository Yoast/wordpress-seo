{
  "name": "yoastseo",
  "description": "Yoast client-side content analysis",
  "homepage": "https://github.com/Yoast/",
  "keywords": [
    "Yoast",
    "SEO",
    "readability",
    "inclusive language"
  ],
  "main": "build/index.js",
  "types": "types",
  "license": "GPL-3.0",
  "version": "2.0.0-alpha.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoast/wordpress-seo.git",
    "directory": "packages/yoastseo"
  },
  "files": [
    "build",
    "!*.map",
    "vendor",
    "images"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn build:js && yarn build:types",
    "build:js": "babel src --copy-files --source-maps --out-dir build",
    "build:types": "tsc",
    "clean": "rm -rf build",
    "test": "grunt get-premium-configuration && jest",
    "lint": "eslint . --max-warnings 8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@yoast/browserslist-config": "workspace:^",
    "blob-polyfill": "^7.0.20220408",
    "console.table": "^0.10.0",
    "eslint": "^8.57.0",
    "eslint-config-yoast": "workspace:^",
    "eslint-plugin-import": "^2.29.1",
    "grunt": "^1.6.1",
    "grunt-shell": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-loader": "^1.0.0",
    "js-yaml": "^4.1.0",
    "load-grunt-config": "^1.0.0",
    "typescript": "^5.6.3"
  },
  "bugs": {
    "url": "https://github.com/Yoast/wordpress-seo/issues"
  },
  "dependencies": {
    "@wordpress/i18n": "^4.51.1",
    "@yoast/feature-flag": "workspace:^",
    "htmlparser2": "^3.9.2",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "parse5": "^7.1.2",
    "tiny-segmenter": "^0.2.0",
    "tokenizer2": "^2.0.1"
  },
  "yoast": {
    "premiumConfiguration": ""
  }
}
