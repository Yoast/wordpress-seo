{
  "name": "@yoast/ui-library",
  "version": "4.0.0",
  "description": "Yoast UI library",
  "main": "build/index.js",
  "style": "build/css/style.css",
  "files": [
    "build",
    "!*.map"
  ],
  "author": "Team Yoast <support@yoast.com>",
  "license": "GPL-3.0",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yoast/wordpress-seo.git",
    "directory": "packages/ui-library"
  },
  "scripts": {
    "build": "yarn clean && yarn build:js && yarn build:css",
    "build:js": "babel src --out-dir build --ignore \"src/**/*.stories.js\",\"src/**/stories.js\",\"src/**/docs/**/*.js\"",
    "build:css": "yarn node ./scripts/build-css.js",
    "clean": "rm -rf build",
    "watch": "yarn watch:js & yarn watch:css",
    "watch:js": "yarn build:js --watch",
    "watch:css": "yarn node ./scripts/watch-css.js",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "publish:storybook": "./scripts/publish-storybook.sh",
    "test:storyshots": "yarn build:storybook --test --quiet && jest tests/storyshots.js",
    "t_st": "yarn test:storyshots",
    "lint": "eslint . --max-warnings=0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/eslint-plugin": "^7.23.5",
    "@babel/parser": "^7.24.4",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@mdx-js/react": "^2.1.5",
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/addons": "^7.6.17",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preview-api": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@storybook/theming": "^7.6.17",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@wordpress/jest-preset-default": "^11.27.0",
    "@yoast/browserslist-config": "workspace:^",
    "@yoast/jest-preset": "workspace:^",
    "@yoast/postcss-preset": "workspace:^",
    "@yoast/tailwindcss-preset": "workspace:^",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "eslint": "^8.57.0",
    "eslint-config-yoast": "workspace:^",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "puppeteer": "^17.1.3",
    "storybook": "^7.6.17",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.88.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^1.0.6",
    "@reduxjs/toolkit": "^1.8.3",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-animate-height": "^3.1.0",
    "react-error-boundary": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
