{
  "name": "yoast-wordpress-seo",
  "version": "3.0",
  "homepage": "https://github.com/Yoast/wordpress-seo",
  "authors": [
    "Yoast.com"
  ],
  "dependencies": {
    "yoast-seo": "https://github.com/Yoast/YoastSEO.js.git"
  },
  "install": {
    "path": {
      "css": "css/dist",
      "js": "js/dist",
      "png": "css/dist"
    },
    "sources": {
      "yoast-seo": {
        "mapping": [
          {"bower_components/yoast-seo/dist/yoast-seo.min.css": "yoast-seo-302.min.css"},
          {"bower_components/yoast-seo/dist/images/editicon.png": "editicon.png"},
          {"bower_components/yoast-seo/dist/yoast-seo.min.js": "yoast-seo-302.min.js"}
        ]
      }
    }
  },
  "description": "Yoast SEO for WordPress",
  "keywords": [
    "SEO",
    "WordPress",
    "Text analysis"
  ],
  "license": "GPLv3"
}
